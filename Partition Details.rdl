            <Value>=Parameters!ObjectName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ObjectTypeName">
            <Value>=Parameters!ObjectTypeName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
